{
  "name": "go-starter-tool",
  "displayName": "Go Starter Tool",
  "description": "A starter tool for building Go applications",
  "version": "0.1.2",
  "publisher": "AhmadFathan",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "go-starter-tool.helloWorld",
        "title": "Go Starter Tool: Hello World"
      },
      {
        "command": "go-starter-tool.newProject",
        "title": "Go Starter Tool: New Project"
      },
      {
        "command": "go-starter-tool.newController",
        "title": "Go Starter Tool: New Controller"
      },
      {
        "command": "go-starter-tool.newUseCase",
        "title": "Go Starter Tool: New Use Case"
      },
      {
        "command": "go-starter-tool.newRepository",
        "title": "Go Starter Tool: New Repository"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.107.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1"
  },
  "files": [
    "dist/",
    "templates/**"
  ],
  "dependencies": {
    "fs-extra": "^11.3.3"
  }
}